<head>
<script type="text/javascript">
function submit() {
		var name = $("#name").val();
		var sex = $("#sex").val();
		var age = $("#age").val();
		var phone = $("#phone").val();
		var email = $("#email").val();
		 if (name.length == 0 || name.length > 50) {
			$("#name").focus();
			return;
		}
		if (phone.length == 0 || email.length > 50) {
			$("#email").focus();
			return;
		}
		if (phone.length == 0 || phone.length > 30) {
			$("#phone").focus();
			return;
		}
		$.ajax({
			url : "rightSubmit.htm",
			type : "post",
			data : {
				"name" : name,
				"phone" : phone,
				"email" : email,
				"sex" : sex,
				"age" : age
			},
			dataType : "text",
			success : function(data) {
				$("#name").val("");
				$("#email").val("");
				$("#phone").val("");
				$("#age").attr("value","1");
				$("#sex").attr("value","1");
				alert('您好,我们已收到您的预约申请,请等候工作人员与您联系!');
			}
		});
	}
</script>
