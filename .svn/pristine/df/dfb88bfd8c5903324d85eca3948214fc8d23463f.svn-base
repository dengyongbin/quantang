<script type="text/javascript" charset="utf-8">
	$(document).ready(function() {
		//var isRetina = window.devicePixelRatio > 1 ? true : false;
		GetIDInfo();
		$("#article1").mouseover(function(){
			  $("#info1").show();
		});
		$("#article2").mouseover(function(){
			  $("#info2").show();
		});
		$("#article3").mouseover(function(){
			  $("#info3").show();
		});
		$("#article1").mouseout(function(){
			  $("#info1").hide();
		});
		$("#article2").mouseout(function(){
			  $("#info2").hide();
		});
		$("#article3").mouseout(function(){
			  $("#info3").hide();
		});
		$("#editerform").submit(function(){
			var options ={
					dataType:'json',  
                    resetForm:false,  
                    success:showSuccess,  
                    error:showError,  
                    timeout:10000  
			}
			 $(this).ajaxSubmit(options);  
			return false;  
		});
	});
	function GetIDInfo() {
		$.ajax({
			url : "getWTI.htm",
			type : "post",
			dataType : "text",
			success : function(data) {
				if (data != "") {
					var array = new Array();
					array = data.split(",");
					if (array.length >= 10) {
						/* $("#wtitime").html(array[0]); */
						$("#newsprice").html(array[1]);
						$("#openprice").html(array[2]);
						$("#gains").html(array[3]);
						$("#gainspercent").html(array[4]);
						$("#highestprice").html(array[5]);
						$("#saleprice").html(array[6]);
						$("#yesterdayprice").html(array[7]);
						$("#lowestprice").html(array[8]);
						$("#buyprice").html(array[9]);
					};
				};
				setTimeout("GetIDInfo()", 1000);
			}
		});
	}
	
	function submit() {
		var name = $("#name").val();
		var sex = $("#sex").val();
		var age = $("#age").val();
		var phone = $("#phone").val();
		var email = $("#email").val();
		 if (name.length == 0 || name.length > 50) {
			$("#name").focus();
			return;
		}
		if (phone.length == 0 || email.length > 50) {
			$("#email").focus();
			return;
		}
		if (phone.length == 0 || phone.length > 30) {
			$("#phone").focus();
			return;
		}
		$.ajax({
			url : "rightSubmit.htm",
			type : "post",
			data : {
				"name" : name,
				"phone" : phone,
				"email" : email,
				"sex" : sex,
				"age" : age
			},
			dataType : "text",
			success : function(data) {
				$("#name").val("");
				$("#email").val("");
				$("#phone").val("");
				$("#age").attr("value","1");
				$("#sex").attr("value","1");
				alert('您好,我们已收到您的预约申请,请等候工作人员与您联系!');
			}
		});
	}
</script>
