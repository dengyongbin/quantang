<script type="text/javascript">
	$(document).ready(function() {
		$("#subscribe").click(function() {
			var email = $("#ne").val();
			if (email.length == 0 || email == 'Email') {
				$("#ne").focus();
				return;
			}
			$.ajax({
				url : "subscribe.htm",
				type : "post",
				data : {
					"email" : email
				},
				dataType : "text",
				success : function(data) {
					$("#ne").val("Email");
					alert('订阅成功!');
				}
			});
		});
	});
</script>
